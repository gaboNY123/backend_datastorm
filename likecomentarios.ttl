@prefix : <http://example.org/ontology#> .
@prefix user: <http://example.org/usuario#> .
@prefix comentario: <http://example.org/comentario#> .

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dbo: <http://dbpedia.org/ontology/> .

<http://example.org/likecomentarios>
    a owl:Ontology ;
    owl:imports 
        <http://example.org/usuario> ,
        <http://example.org/comentario> .

:userDaLikeComentario a owl:ObjectProperty ;
    rdfs:domain user:Usuario ;
    rdfs:range comentario:Comentario ;
    rdfs:label "dio like al comentario" ;
    rdfs:comment "El usuario dio like a un comentario" ;
    owl:equivalentProperty dbo:like .
